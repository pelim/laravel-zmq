{
    "name": "pelim/laravel-zmq",
    "description": "A ZeroMQ broadcast driver for Laravel 5-6",
    "license": "MIT",
    "authors": [
        {
            "name": "pelim",
            "email": "peter.limbach@gmail.com"
        }
    ],
    "require": {
        "ext-zmq": "*",
        "illuminate/broadcasting": "^5.5|^6.0",
        "illuminate/contracts": "^5.5|^6.0",
        "illuminate/http": "^5.5|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Pelim\\LaravelZmq\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pelim\\LaravelZmq\\ZmqServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
